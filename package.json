        "menus": {
            "view/title": [
                {
                    "command": "webososeDevices.refreshList",
                    "when": "view == webososeDevices",
                    "group": "navigation"
                },
                {
                    "command": "webososeDevices.addDevice",
                    "when": "view == webososeDevices",
                    "group": "navigation"
                },
                {
                    "command": "apps.projectWizard",
                    "when": "view == apps",
                    "group": "navigation"
                },
                {
                    "command": "apps.import",
                    "when": "view == apps",
                    "group": "navigation"
                },
                {
                    "command": "apps.refreshList",
                    "when": "view == apps",
                    "group": "navigation"
                },
                {
                    "command": "vbox.refreshList",
                    "when": "view == vbox",
                    "group": "navigation"
                },
                {
                    "command": "vbox.addInstance",
                    "when": "view == vbox",
                    "group": "navigation"
                },
                {
                    "command": "webosSimulator.refreshList",
                    "when": "view == webosSimulator",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "webososeDevices.modifyDevice",
                    "when": "view == webososeDevices && viewItem == device",
                    "group": "inline"
                },
                {
                    "command": "webososeDevices.removeDevice",
                    "when": "view == webososeDevices && viewItem == device",
                    "group": "inline"
                },
                {
                    "command": "webososeDevices.refreshDevice",
                    "when": "view == webososeDevices && viewItem == device",
                    "group": "device1@1"
                },
                {
                    "command": "webososeDevices.installApp",
                    "when": "view == webososeDevices && viewItem == device",
                    "group": "device2@2"
                },
                {
                    "command": "webososeDevices.setDefaultDevice",
                    "when": "view == webososeDevices && viewItem == device",
                    "group": "device2@5"
                },
                {
                    "command": "webososeDevices.getDeviceInfo",
                    "when": "view == webososeDevices && viewItem == device",
                    "group": "device2@6"
                },
                {
                    "submenu": "webososeDevices.runApp",
                    "when": "webos.profile == 'OSE' && view == webososeDevices && viewItem == installed || webos.profile == 'OSE' && view == webososeDevices && viewItem == running",
                    "group": "app1@1"
                },
                {
                    "command": "webososeDevices.runApp",
                    "when": "webos.profile == 'TV' && view == webososeDevices && viewItem == installed || webos.profile == 'TV' && view == webososeDevices && viewItem == running",
                    "group": "app1@1"
                },
                {
                    "command": "webososeDevices.uninstallApp",
                    "when": "view == webososeDevices && viewItem == installed",
                    "group": "app2@3"
                },
                {
                    "submenu": "webososeDevices.closeApp",
                    "when": "webos.profile == 'OSE' && view == webososeDevices && viewItem == running",
                    "group": "app2@3"
                },
                {
                    "command": "webososeDevices.closeApp",
                    "when": "webos.profile == 'TV' && view == webososeDevices && viewItem == running",
                    "group": "app2@3"
                },
                {
                    "command": "apps.packageApp",
                    "when": "view == apps && viewItem == web-app || viewItem == enact-app",
                    "group": "app1@1"
                },
                {
                    "command": "apps.installApp",
                    "when": "view == apps && viewItem == web-app || viewItem == enact-app",
                    "group": "app1@2"
                },
                {
                    "command": "apps.runApp",
                    "when": "view == apps && viewItem == web-app || viewItem == enact-app",
                    "group": "app1@3"
                },
                {
                    "command": "webos.runSimulator",
                    "when": "view == apps && viewItem == web-app || viewItem == enact-app",
                    "group": "app1@4"
                },
                {
                    "command": "apps.removeApp",
                    "when": "view == apps && viewItem == web-app || viewItem == enact-app || viewItem == js-service",
                    "group": "app1@3"
                },
                {
                    "command": "apps.installWebOS",
                    "when": "view == apps && viewItem == js-service || viewItem == enact-app",
                    "group": "app2@2"
                },
                {
                    "command": "apps.previewApp",
                    "when": "view == apps && viewItem == web-app || viewItem == enact-app",
                    "group": "app2@2"
                },
                {
                    "command": "apps.runWithoutInstall",
                    "when": "view == apps && viewItem == web-app || viewItem == enact-app",
                    "group": "app2@2"
                },
                {
                    "submenu": "apps.debugApp",
                    "when": "view == apps && viewItem == js-service || viewItem == enact-app || viewItem == web-app",
                    "group": "app2@2"
                },
                {
                    "command": "apps.lintEnactApp",
                    "when": "view == apps && viewItem == enact-app",
                    "group": "app2@2"
                },
                {
                    "command": "apps.lintEnactAppDisable",
                    "when": "view == apps && viewItem == enact-app",
                    "group": "app2@2"
                },
                {
                    "command": "apps.lintWebApp",
                    "when": "view == apps && viewItem == hidden-menu",
                    "group": "app2@2"
                },
                {
                    "command": "apps.lintWebAppDisable",
                    "when": "view == apps && viewItem == hidden-menu",
                    "group": "app2@2"
                },
                {
                    "command": "apps.runApp",
                    "when": "view == apps && viewItem == web-app || viewItem == enact-app",
                    "group": "inline"
                },
                {
                    "command": "apps.removeApp",
                    "when": "view == apps && viewItem == web-app || viewItem == enact-app || viewItem == js-service",
                    "group": "inline"
                },
                {
                    "command": "vbox.modifyInstance",
                    "when": "view == vbox && viewItem == vboxInsance",
                    "group": "inline"
                },
                {
                    "command": "vbox.removeInstance",
                    "when": "view == vbox && viewItem == vboxInsance",
                    "group": "inline"
                },
                {
                    "command": "vbox.refreshList",
                    "when": "view == vbox && viewItem == vboxInsance",
                    "group": "device1@1"
                },
                {
                    "command": "vbox.launchInstance",
                    "when": "view == vbox && viewItem == vboxInsance",
                    "group": "device2@2"
                },
                {
                    "command": "webosSimulator.runApp",
                    "when": "view == webosSimulator && viewItem == simulator"
                }
            ],
            "explorer/context": [
                {
                    "command": "webosose.explorer.debugipk",
                    "when": "resourceExtname == .ipk",
                    "group": "device2@2"
                },
                {
                    "command": "webosose.explorer.installApp",
                    "when": "resourceExtname == .ipk",
                    "group": "device2@1"
                },
                {
                    "command": "webosose.explorer.runApp",
                    "when": "resourceExtname == .ipk",
                    "group": "device2@1"
                },
              
                {
                    "command": "webosose.explorermenu.packageApp",
                    "when": "explorerResourceIsFolder && resourcePath in webosose.supportedAppFolders",
                    "group": "device1@1"
                },
                {
                    "command": "webosose.explorermenu.installApp",
                    "when": "explorerResourceIsFolder && resourcePath in webosose.supportedAppFolders",
                    "group": "device1@2"
                },
                {
                    "command": "webosose.explorermenu.runApp",
                    "when": "explorerResourceIsFolder && resourcePath in webosose.supportedAppFolders",
                    "group": "device1@3"
                },
                {
                    "command": "webosose.explorermenu.runSimulator",
                    "when": "explorerResourceIsFolder && resourcePath in webosose.supportedAppFolders",
                    "group": "device1@4"
                },
                {
                    "command": "webosose.explorermenu.appPreview",
                    "when": "explorerResourceIsFolder && resourcePath in webosose.supportedAppFolders",
                    "group": "device2@1"
                },
                {
                    "command": "webosose.explorermenu.runWithoutInstall",
                    "when": "explorerResourceIsFolder && resourcePath in webosose.supportedAppFolders",
                    "group": "device2@1"
                },
                {
                    "submenu": "webosose.explorermenu.debug",
                    "when": "explorerResourceIsFolder && resourcePath in webosose.supportedFolders",
                    "group": "device2@1"
                }
            ],
            "commandPalette": [
                {
                    "command": "webosose.explorer.installApp",
                    "when": "false"
                },
                {
                    "command": "webosose.explorer.runApp",
                    "when": "false"
                },
                {
                    "command": "webosose.explorer.debugipk",
                    "when": "false"
                },
                {
                    "command": "webosose.explorermenu.appPreview",
                    "when": "false"
                },
                {
                    "command": "webosose.explorermenu.runWithoutInstall",
                    "when": "false"
                },
                {
                    "command": "vbox.refreshList",
